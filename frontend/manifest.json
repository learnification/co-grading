{
  "manifest_version": 3,
  "name": "FeedbackAI",
  "description": "A tool to help educators grade assignments in Canvas.",
  "version": "1.0.0",
  "options_ui": {
    "page": "src/pages/options/index.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "48": "images/icon48.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-DYiAg6UP.js"
      ],
      "matches": [
        "https://*/courses/*/assignments/*"
      ],
      "css": [
        "assets/createLucideIcon-CRgOeeIc.css"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://*/*"
      ],
      "resources": [
        "assets/createLucideIcon-Ck8SqCQ8.js",
        "assets/Textarea-DcKjW1V4.js",
        "assets/grading-BF2zApe_.js",
        "assets/actions-DCwID940.js",
        "assets/CanvasService-BNUUX2rn.js",
        "assets/GradingRepository-DZXIIiP0.js",
        "assets/index.tsx-Dsd8HkZn.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
